from selenium import webdriver
from selenium.webdriver.chrome.options import Options
from selenium.webdriver.common.by import By


options = Options()
options.chrome_executable_path = "/Users/denny89928/程式設計/網路爬蟲/chromedriver-mac-arm64/chromedriver"
driver = webdriver.Chrome(options=options)


driver.get("https://ptt-info.tw/bbs/sex/index.html")
#print(driver.page_source) 可以取得原始碼
tags = driver.find_elements(By.CLASS_NAME,"title")
for tag in tags:
    print(tag.text)
link = driver.find_element(By.LINK_TEXT,"‹ 上頁")
link.click()

tags = driver.find_elements(By.CLASS_NAME,"title")
for tag in tags:
    print(tag.text)

driver.close()
